# Bind Spark IP
SPARK_LOCAL_IP="127.0.0.1"

# DBT config
DBT_PROFILES_DIR="directory/path"
DBT_PROJECT_DIR="directory/path"

## LLM Config Zone ##
# LLM Parameter
TEMPERATURE="0.5"
TOP_P="0.5"
TOP_K="5"

# LLM Model
LLM_MODEL="typhoon-v2-70b-instruct"
LLM_BASE_URL="https://api.opentyphoon.ai/v1"
